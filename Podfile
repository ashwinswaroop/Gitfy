# platform :ios, '9.0'

target 'Gitfy' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'p2.OAuth2', :git => 'https://github.com/p2/OAuth2', :submodules => true
  pod "OcticonsKit"
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'

end
